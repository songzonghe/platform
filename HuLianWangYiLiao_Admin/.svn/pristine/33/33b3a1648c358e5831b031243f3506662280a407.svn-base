<%@ page language="java" pageEncoding="utf-8"%> 
<%@include file="/form_util/page/root.jsp"%>
<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<div class="widget-main no-padding" style="width: 880px">
	<form class="form-horizontal" isValidate="y" onsubmit="return false;">
		<div class="form-group text-center">
			<font size="4"> <strong></strong> </font>
		</div>
		
		
		<div class="row">
			<div class="form-group col-sm-6 field">
				<label class="col-sm-4 control-label no-padding-right">
					设备代号
				</label>
				<div class="col-sm-6 field">
					<span class="block input-icon input-icon-right"> 
							<input type="text" placeholder="设备代号" id="update_equipment_sbdh" class="col-xs-12" data-validate="required:设备代号不能为空哦！">
					</span>
				</div>
			</div>
			<div class="form-group col-sm-6 field">
				<label class="col-sm-2 control-label no-padding-right">
					设备名称
				</label>
				<div class="col-sm-6 field">
					<span class="block input-icon input-icon-right"> 
							<input type="text" placeholder="设备名称" id="update_equipment_sbmc" class="col-xs-12" data-validate="required:设备名称不能为空哦！">
					</span>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="form-group col-sm-6 field">
				<label class="col-sm-4 control-label no-padding-right">
					设备台数
				</label>
				<div class="col-sm-6 field">
					<span class="block input-icon input-icon-right"> 
							<input type="text" placeholder="设备台数" id="update_equipment_tpsbts" class="col-xs-12" data-validate="required:设备台数不能为空哦！">
					</span>
				</div>
			</div>
			<div class="form-group col-sm-6 field">
				<label class="col-sm-2 control-label no-padding-right">
					产地
				</label>
				<div class="col-sm-6 field">
					<span class="block input-icon input-icon-right"> 
						<select id="update_equipment_cd" class="col-xs-12" data-validate="required:不能为空哦！" >
							<option value="">请选择</option>
							<option value="1" <c:if test="${map.equipment_cd=='1'}">selected="selected"</c:if>>进口</option>
							<option value="2" <c:if test="${map.equipment_cd=='2'}">selected="selected"</c:if>>国产</option>
							<option value="3" <c:if test="${map.equipment_cd=='3'}">selected="selected"</c:if>>合资</option>
							<option value="9" <c:if test="${map.equipment_cd=='9'}">selected="selected"</c:if>>未知</option>
						</select>
					</span>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="form-group col-sm-6 field">
				<label class="col-sm-4 control-label no-padding-right">
					生产厂家
				</label>
				<div class="col-sm-6 field">
					<span class="block input-icon input-icon-right"> 
							<input type="text" placeholder="生产厂家" id="update_equipment_sccj" class="col-xs-12" data-validate="required:生产厂家不能为空哦！">
					</span>
				</div>
			</div>
			<div class="form-group col-sm-6 field">
				<label class="col-sm-2 control-label no-padding-right">
					设备型号
				</label>
				<div class="col-sm-6 field">
					<span class="block input-icon input-icon-right"> 
							<input type="text" placeholder="设备型号" id="update_equipment_sbxh" class="col-xs-12" data-validate="required:设备型号不能为空哦！">
					</span>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="form-group col-sm-6 field">
				<label class="col-sm-4 control-label no-padding-right">
					设备参数
				</label>
				<div class="col-sm-6 field">
					<span class="block input-icon input-icon-right"> 
							<input type="text" placeholder="设备参数" id="update_equipment_sbcs" class="col-xs-12" data-validate="required:设备参数不能为空哦！">
					</span>
				</div>
			</div>
			<div class="form-group col-sm-6 field">
				<label class="col-sm-2 control-label no-padding-right">
					设备类型
				</label>
				<div class="col-sm-6 field">
					<span class="block input-icon input-icon-right"> 
							<input type="text" placeholder="设备类型" id="update_equipment_sblx" class="col-xs-12" data-validate="required:设备类型不能为空哦！">
					</span>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="form-group col-sm-6 field">
				<label class="col-sm-4 control-label no-padding-right">
					日期
				</label>
				<div class="col-sm-6 field">
					<span class="block input-icon input-icon-right"> 
						<input type="text" placeholder="日期" id="update_equipment_gmrq" class="col-xs-12 Wdate" data-validate="required:日期不能为空哦！" readonly="readonly" onclick="WdatePicker({dateFmt:'yyyy-MM-dd'})" >
					</span>
				</div>
			</div>
			<div class="form-group col-sm-6 field">
				<label class="col-sm-2 control-label no-padding-right">
					用途
				</label>
				<div class="col-sm-6 field">
					<span class="block input-icon input-icon-right"> 
							<input type="text" placeholder="用途" id="update_equipment_yt" class="col-xs-12" data-validate="required:用途不能为空哦！">
					</span>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="form-group col-sm-6 field">
				<label class="col-sm-4 control-label no-padding-right">
					设备价值金额
				</label>
				<div class="col-sm-6 field">
					<span class="block input-icon input-icon-right"> 
							<input type="text" placeholder="设备价值金额" id="update_equipment_sbjzje" class="col-xs-12" data-validate="required:设备价值金额不能为空哦！">
					</span>
				</div>
			</div>
			<div class="form-group col-sm-6 field">
				<label class="col-sm-2 control-label no-padding-right">
					新旧情况
				</label>
				<div class="col-sm-6 field">
					<span class="block input-icon input-icon-right"> 
							<input type="text" placeholder="新旧情况" id="update_equipment_gjsxqk" class="col-xs-12" data-validate="required:新旧情况不能为空哦！">
					</span>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="form-group col-sm-6 field">
				<label class="col-sm-4 control-label no-padding-right">
					设计寿命
				</label>
				<div class="col-sm-6 field">
					<span class="block input-icon input-icon-right"> 
							<input type="text" placeholder="设计寿命" id="update_equipment_llsjsm" class="col-xs-12" data-validate="required:设计寿命不能为空哦！">
					</span>
				</div>
			</div>
			<div class="form-group col-sm-6 field">
				<label class="col-sm-2 control-label no-padding-right">
					使用情况
				</label>
				<div class="col-sm-6 field">
					<span class="block input-icon input-icon-right"> 
						<select id="update_equipment_syqk" class="col-xs-12" data-validate="required:不能为空哦！" >
							<option value="">请选择</option>
							<option value="1" <c:if test="${map.equipment_syqk=='1'}">selected="selected"</c:if>>启用</option>
							<option value="2" <c:if test="${map.equipment_syqk=='2'}">selected="selected"</c:if>>未启用</option>
							<option value="3" <c:if test="${map.equipment_syqk=='3'}">selected="selected"</c:if>>报废</option>
						</select> 
					</span>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="form-group col-sm-6 field">
				<label class="col-sm-4 control-label no-padding-right">
					机构代码
				</label>
				<div class="col-sm-6 field">
					<span class="block input-icon input-icon-right"> 
						<input type="text" placeholder="机构代码" id="update_equipment_jgdm" class="col-xs-12" value="${map.equipment_jgdm}" data-validate="required:机构代码能为空哦！">
					</span>
				</div>
			</div>
			<div class="form-group col-sm-6 field">
				<label class="col-sm-2 control-label no-padding-right">
					撤销标志
				</label>
				<div class="col-sm-6 field">
					<span class="block input-icon input-icon-right"> 
						<select id="update_equipment_cxbz" class="col-xs-12" data-validate="required:不能为空哦！" >
							<option value="">请选择</option>
							<option value="1" <c:if test="${map.equipment_cxbz=='1'}">selected="selected"</c:if>>正常</option>
							<option value="2" <c:if test="${map.equipment_cxbz=='2'}">selected="selected"</c:if>>已撤销</option>
						</select> 
					</span>
				</div>
			</div>
		</div>
		
		<div class="row">
			<div class="form-group col-sm-12">
				<label class="col-sm-2 control-label no-padding-right" style="margin-left: -5px;">
					备注
				</label>
				<div class="col-sm-10">
					<textarea style="width: 81%;" id="beiZhu" placeholder="备注"></textarea>
				</div>
			</div>
		</div>
		<div class="form-actions center">
			<button type="button" class="btn btn-sm btn-lg" onclick="closeShowDiv()">
				返回
				<i class="ace-icon fa fa-arrow-left icon-on-right bigger-110"></i>
			</button>
			<button type="submit" class="btn btn-sm btn-success" onclick="addDataValidate()">
				提交
				<i class="ace-icon fa fa-arrow-right icon-on-right bigger-110"></i>
			</button>
		</div>
	</form>
</div>
<script>
	initValidate();
</script>
