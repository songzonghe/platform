<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">
<taglib>
	<tlibversion>1.2</tlibversion>
	<jspversion>1.1</jspversion>
	<shortname>tagTest</shortname>
	<uri>/tagUrl</uri>
	<!-- 将map转换整json字符串 -->
	<tag>
		<name>mapToJson</name>
		<tagclass>com.custom.tag.MyTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>map</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<!--  将list转换整json字符串 -->
	<tag>
		<name>listToStr</name>
		<tagclass>com.custom.tag.MyTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>listMap</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<!--  计算百分比 -->
	<tag>
		<name>perDouble</name>
		<tagclass>com.custom.tag.MyTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>divisor</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>dividend</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
	<!--  将空字符串 转化成0 -->
	<tag>
		<name>emptyToZero</name>
		<tagclass>com.custom.tag.MyTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>toZero</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib> 